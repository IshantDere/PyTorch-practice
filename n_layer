from torch import nn
import torch

lists = []
n_layers = 10
in_channels = 3
hidden_dim = 32
out_channels = 3
for i in range(n_layers):
    lists.append(nn.Conv2d(in_channels,
                           hidden_dim, (3,3),(1,1),1))
    in_channels = hidden_dim
    hidden_dim = hidden_dim * 2

print(lists)
